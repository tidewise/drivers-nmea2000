    "version": "0.2.0",
    "configurations": [
        {
            "name": "(drivers/nmea2000) nmea2000_actisense_ctl",
            "type": "cppdbg",
            "request": "launch",
            "program": "${rock:which:nmea2000_actisense_ctl}",
            "args": ["serial:///dev/ttyUSB0:115200", "log"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(drivers/nmea2000) test suite",
            "type": "cppdbg",
            "request": "launch",
            "program": "${rock:buildDir}/test/test_suite",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}